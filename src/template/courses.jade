extends layout

mixin kurs (date,p,title,h,img)
	article.col-2.curs.open
		.curs-img
			img.img-responsive(src="#{img}", alt="#{title}")
		.clearfix
			.float-l.small Учебный курс
			.float-r.curs-h!= h+' часов'
		h2: a(href="#",title="#{title}")!=title
		p.small.curs-desk!=p
		.clearfix.curs-bottom
			.float-l Цену уточняйте
			.float-r!=date
mixin kursPlans (p,title,h,img)
	article.col-2.curs.plan
		.curs-img
			img.img-responsive(src="#{img}",alt="#{title}")
		.clearfix
			.float-l.small Учебный курс
			.float-r.curs-h!= h+' часов'
		h2!=title
		p.small.curs-desk!=p
		.clearfix.curs-bottom
			button.float-l.curs-showform(type="button") Подписаться
			.float-r СКОРО

block content
	h1.top Курсы и продукты
	.grid
		+kurs('26 октября','С нуля, за 3 месяца, уделяя 2–3 часа  в день, до уровня устройства на работу','Профессия — дизайнер сайтов',90,'data:image/png;base64,')
		+kursPlans('Профессиональный курс по дизайну пользовательских интерфейсов','Мастерство UI-дизайна',60,'data:image/png;base64,')
		+kursPlans('Профессиональный курс по дизайну пользовательских интерфейсов','Мастерство UI-дизайна',60,'data:image/png;base64,')
		+kursPlans('Все о разработке дизайна графического интерфейса мобильных приложений','Яндекс.Директ',10,'data:image/png;base64,')
		+kursPlans('Все о разработке дизайна графического интерфейса мобильных приложений','UI-дизайн мобильных приложений',3,'data:image/png;base64,')
		+kursPlans('Профессиональный курс по дизайну пользовательских интерфейсов ','Прототи-пирование в Axure',30,'data:image/png;base64,')
		+kursPlans('Профессиональный курс по дизайну пользовательских интерфейсов','Исскуство продажи веб-сайтов',30,'data:image/png;base64,')
		+kursPlans('Все о разработке дизайна графического интерфейса мобильных приложений ','Google.Adwords',10,'data:image/png;base64,')
		+kursPlans('Все о разработке дизайна графического интерфейса мобильных приложений ','Маркетинг в социальных сетях',90,'data:image/png;base64,')
		.gap
		.gap


	// Форма для клонирования т.е при клике "ПОДПИСАТЬСЯ" она встраивается в нужный нам блок, 
		динамически добавляется скрытое поле 'curs' которое хранит название курса,
		В js авторитически парсит атрибуты action,post и отправляет запрос на указаный в форме action! 
		Данные:
			name -
			email -
			curs -
	form.curs-form.clone-form(action="/post",method='post',autocomplete="on",style="display:none")
		h3.curs-form-info Учебный курс в разработке
		p.curs-form-desk Подпишитесь, чтобы узнать первым о начале записи и скидках
		p.curs-form-done Поздравляем!<br>Вы успешно подписались
		input.curs-form-input(type="text",name="name",placeholder='ВАШЕ ИМЯ',required)
		input.curs-form-input(type="email",name="email",placeholder='E-MAIL',required)
		button.curs-form-submit(type='submit') Подписаться
		button.curs-form-hide(type='button') Не подписываться